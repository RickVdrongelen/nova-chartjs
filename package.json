        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "chart.js": "^2.9.3",
        "chartjs-plugin-datalabels": "^0.7.0",
        "serialize-javascript": "^4.0.0",
        "sweetalert2": "^9.17.2",
